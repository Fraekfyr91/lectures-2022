      "n =   2: 3.8600000000\n",
      "n =   3: 3.8600000000\n",
      "n =   5: 3.8600000000\n",
      "n =   7: 3.8600000000\n",
      "n =   9: 3.8600000000\n",
      "n =  11: 3.8600000000\n"
     ]
